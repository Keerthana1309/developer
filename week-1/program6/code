/*Given a non-empty array of integers, every element appears three times except for one, which appears exactly once. Find that single one*/
import java.io.*;
import java.util.*;
class Main 
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(new File("week-1/program6/Input"));
        int n=sc.nextInt();
        int[] array=new int[n];
        for(int i=0;i<n;i++)
            array[i]=sc.nextInt();
        int res=SingleElement(array);
        System.out.println(res);
    }
    public static int SingleElement(int[] array)
    {
        int FirstTime=0,SecondTime=0;
        for(int i=0;i<array.length;i++)
        {
            SecondTime|=FirstTime&array[i];
            FirstTime^=array[i];
            int SetToZero=~(FirstTime&SecondTime);
            FirstTime&=SetToZero;
            SecondTime&=SetToZero;
        }
        return FirstTime;
    }
}
